html body {
  margin:0;
  padding:0;
  background:#F6F6F6 url("../img/sides_body.gif") repeat-y center top;  			 <!-- background color on the whole-->
  text-align:center;
  font:small "Trebuchet MS",Verdana,Arial,Sans-serif;
  color:#333;
  }


/* Page Structure
----------------------------------------------- */

#body {
  width:80%;
  padding:10px 0 15px;
  margin:0 auto;
  text-align:left;
  }
#main {
  background:#fff url('../img/c_top.gif') no-repeat left top;
  padding-top:15px;
  float:left;
  width:100%
  }
#m2 {
  background:url('../img/c_bot.gif') no-repeat left bottom;
  padding-bottom:15px;
  float:left;
  width:100%
  }
#m3 {
  border:5px solid #6699CC;				<!-- left/right border-->
  border-width:0 5px;
  padding:0 15px;
  padding-top:15px;
  padding-bottom:15px;
  float:left;
  width:95%;
  }


/* Links
----------------------------------------------- */
a {
  font-weight:bold;
  color:#002255;
  }
a:hover {
  text-decoration:underline;
  color:#FF6600;
  }
a img {
  border-width:0;
  }


/* header
----------------------------------------------- */
#header {
  width:80%;
  padding:10px 0 15px;
  margin:0 auto;
  text-align:left;
  }

#header #logo {
  float:center;
  margin:0;
  padding:0;
  text-decoration:none;
  }
  
#logo img {
  display:block;
  width:150px;
  color:#fff;
  font-size:180%;
  }


/* menu
----------------------------------------------- */
#menu {
  width:100%;
  clear:both;
  padding: 0 0 0;
  }
#menu div {
  background:#6699CC;
  width:100%;
  padding: 10px 0 15px;
  margin:0 auto;
  text-align:center;
  }

#menu ul {
border: 0px none;
margin: 0;
padding: 0;
list-style-type: none;
text-align: center;
}

#menu ul li {
float: left;
text-align: center;
padding: 0;
margin: 0;
}

#menu ul li a {
background:#6699CC;
height: 1.5em;
border-top: 1px solid #ccc;
border-left: 1px solid #ccc;
border-bottom: 1px solid #ccc;
border-right: medium none;
padding-right: 1em;
padding-left: 1em;
color:#002255;
text-decoration: none;
display: block;
text-align: center;
font-weight: wider;
letter-spacing: 1px;
line-height: 2em;
font-size: large;      
voice-family: "\"}\"";   
voice-family: inherit;   
font-size: normal; 
//margin-left:1px; 
//margin-right:0; 
//margin-top:0; 
//margin-bottom:10px   
//min-height:2px;
//margin:0;
//width: 100% ;
color:#002255;
text-align:center;
font-size:93%;
line-height:1.3em;
background:#6699CC;
border-right: 1px solid #002255;
border-left: 1px solid #002255;
border-top: 2px solid #002255;
border-bottom: 2px solid #002255;
text-align: center;
font-weight: bold;
letter-spacing: 1px;
}

#menu ul li a:hover {
color: #FF6600;
}


/* Sign in
----------------------------------------------- */
#header form {
  background:url('../img/bg_login.jpg') no-repeat left top;
  float:right;
  width:280px;
  margin:0;
  color:#369;
  font-size:93%;
  line-height:1.2em; padding-left:20px; padding-right:20px; padding-top:12px; padding-bottom:0
  }
#header form h2 {
  margin:0;
  color:#fff;
  font-size:100%;
  }
#header form div {
  float:left;
  width:47%;
  margin:.4em .5em 0 0;
  white-space:nowrap;
  }
#header p {
  float:left;
  width:95%;
  margin:.3em 0 0;
  color:#369;
  }
#header label {
  color:#69c;
  }


/* Titles
----------------------------------------------- */
h1 {
  margin:0 0 15px;
  font-size:220%;
  line-height:1.2em;
  font-weight:normal;
  color:#666;
  }
#home h1 {
  font-size:240%;
  }
h1 strong {
  color:#000;
  }
h1 em {
  font-style:normal;
  font-weight:bold;
  color:#f60;
  }
h2 {
  margin:1em 0 .5em;
  font-size:130%;
  line-height:1.3em;
  color:#826C55;
  font-weight:normal;
  }

#main h2 strong {
  color:#f60;
  }
  
h3 {
  margin:1em 0 .25em;
  padding:.25em 0;
  font-size:93%;
  text-transform:uppercase;
  letter-spacing:.1em;
  color:#BAA68E;
  }
h4 {
  margin:1em 0 .25em;
  font-size:100%;
  }


/*Structured forms
------------------------------------------------*/
#table {
  width: 40%;
}
#t1{
  float:left;
}
#t2{
  float:right;
}

#login_table {
  width: 30%;
}
#login_t1{
  float:left;
}
#login_t2{
  float:right;
}

#eval_table {
  width: 60%;
}
#eval_t1{
  float:left;
}
#eval_t2{
  float:right;
}


/* Tables
----------------------------------------------- */
table {
  background:#ffffff;
  border-width:0;
  font-size:100%;
  color:#333;
  width:100%
  }

td {
  font-size:93%;
  line-height:1.5em;
  }
th {
  background:#fff;
  padding-left:5px;
  font-size:100%;
  color:#664E38;
  text-align:left;
  }

tr {
  background:#EAF2FF;
  padding-left:5px;
  font-size:100%;
  text-align:left;
  }


/*Project
----------------------------------------------- */


/* Miscellaneous
----------------------------------------------- */
p {
  margin:0 0 .75em;
  line-height:1.5em;
  }
p strong {
  color:#000;
  }
ul {
  margin:0 0 1em;
  padding:0;
  list-style:none;
  }

#project ul li {
  width:auto;
  min-width:100px;
  display:block;
  }

li#one {
  no-repeat
  }
li#two { 
  no-repeat
  }
li#three {
   no-repeat
   }
dl {
  margin:0;
  font-size:93%;
  line-height:1.5em;
  }
dt {
  float:left;
  margin-right:.5em;
  }
dd {
  margin:0 0 .75em 0;
  }
table label {
  margin-right:5px;
  }
label:hover {
  cursor:pointer;
  }

  